    "chart.js": "^2.9.3",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mathjs": "^7.0.1",
    "webpack": "^4.43.0",
    "copy-webpack-plugin": "^6.0.1"
  },
  "devDependencies": {
  }
}

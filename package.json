{
  "name": "fuzzy-potato",
  "description": "Neural networks from the basics",
  "version": "0.0.1",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "watch": "npx webpack --watch --info-verbosity verbose --config webpack.config.js",
    "start": "cd dist && set DEBUG=express:* && nodemon src/server.js",
    "model": "node ./src/main.js",
    "clean": "rm -rf dist"
  },

  "dependencies": {
